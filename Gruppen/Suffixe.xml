      <command>{%1}^{%2}</command>
      <toolTip>superscript</toolTip>
      <image>Suffix/superscript.png</image>
      <keys></keys>
    </item>
    <item name="subscript">
      <command>{%1}_{%2}</command>
      <toolTip>subscript</toolTip>
      <image>Suffix/subscript.png</image>
      <keys></keys>
    </item>
    <item name="super+sub">
      <command>{%1}_{%2}^{%3}</command>
